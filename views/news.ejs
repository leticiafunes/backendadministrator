<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Basket Web Admin App - Admin Ads</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link src="../styles/style.css" rel="stylesheet">
  </head>

  <body class="text-center">
   <% ['News'].forEach(table =>{ %>
    <div class="div-container modal-content rounded-6 shadow ">
        <h1 class="h3 mb-3 fw-normal text-white bg-primary"><%= table %>' table</h1>
       
       
        <div class="row row-cols-1 row-cols-md-3 gx-4" id="datos">

          <button onclick="getNews()">Get News</button>
     
        </div>
    </div>
     <% });%>

     <% ['Advertisements'].forEach(table =>{ %>
      <div class="div-container modal-content rounded-6 shadow ">
          <h1 class="h3 mb-3 fw-normal text-white bg-primary"><%= table %>' table</h1>
         
         
          <div class="row row-cols-1 row-cols-md-3 gx-4" id="datos">
  
            <button onclick="getAdvertisements()">Get Advertisements</button>
       
          </div>
      </div>
       <% });%>
       <% ['Users'].forEach(table =>{ %>
        <div class="div-container modal-content rounded-6 shadow ">
            <h1 class="h3 mb-3 fw-normal text-white bg-primary"><%= table %>' table</h1>
           
           
            <div class="row row-cols-1 row-cols-md-3 gx-4" id="datos">
    
              <button onclick="getUsers()">Get Users</button>
         
            </div>
        </div>
         <% });%>
  </body>

  <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
  <script src="/js/news.js"> </script>

</html>